  "scripts": {
    "postinstall": "./tasks/privateDependencies.sh && ./tasks/checkDependencies.js",
    "lint": "./tasks/lint.js",
    "e2e": "protractor protractor_conf.js",
    "vendor": "cross-env NODE_ENV=dev tasks/buildVendor.js",
    "pretest": "del ./build && npm run vendor && cross-env NODE_ENV=test npm run config",
    "test": "cross-env NODE_ENV=test karma start test/karma.conf.js",
    "testwatch": "npm test -- --auto-watch --no-single-run",
    "postbuild:i18n": "npm run i18n",
    "i18n:check": "tasks/noContext",
    "preversion": "rm package-lock.json || echo 'No useless package-lock to remove'",
    "postversion": "git push --tags",
    "config": "cross-env NODE_ENV=dev tasks/setupConfig.js",
    "start": "cross-env NODE_ENV=dev tasks/setupConfig.js",
    "poststart": "npm run vendor && cross-env NODE_ENV=dev && webpack-dev-server --hot --progress",
    "dist": "cross-env NODE_ENV=dist webpack --progress && cross-env NODE_ENV=dist tasks/buildVendor.js",
    "deploy": "del ./dist && cross-env NODE_ENV=dist tasks/deploy.js",
    "postdist": "tasks/vendorCheckSum.sh",
    "releaser:extract": "releaser --config releaser.config.js --token $RELEASER_GH_TOKEN",
    "releaser:unshift": "./tasks/unshiftReleaser.sh",
    "i18n:extract": "tasks/extractI18N.sh",
    "i18n:compile": "tasks/compileI18n.js",
    "i18n:build": "npm run i18n:extract && npm run i18n:compile",
    "posti18n:build": "./tasks/createI18nCache.js && ./tasks/commitI18n.sh",
    "pretty": "prettier --arrow-parens=always --single-quote --tab-width=4 --require-pragma --print-width=150 --no-prose-wrap --write $(find src/app -type f -name '*.js')",
    "precommit": "lint-staged",
    "create": "appComponent"
  },
  "lint-staged": {
    "*.js": [
      "prettier --arrow-parens=always --single-quote --tab-width=4 --require-pragma --print-width=150 --no-prose-wrap --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
